{
    "rewrites": [
      {
        "source": "/files/:path*",
        "destination": "/files/:path*"
      }
    ],
    "headers": [
      {
        "source": "/files/:path*",
        "headers": [
          {
            "key": "Content-Disposition",
            "value": "attachment; filename=eln-brochure.pdf"
          },
          {
            "key": "Content-Type",
            "value": "application/pdf"
          }
        ]
      }
    ]
  }
  